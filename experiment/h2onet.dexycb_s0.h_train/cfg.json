{
    "base": {
        "exp_name": "h2onet.dexycb_s0.h_train",
        "model_dir": "experiment/h2onet.dexycb_s0.h_train"
    },
    "data": {
        "name": "DEX_YCB_HOPE",
        "transforms_type": "h2onet",
        "eval_type": [
            "test"
        ],
        "input_img_shape": [
            128,
            128
        ],
        "root_dir": "data/DexYCB",
        "data_split": "s0",
        "load_nori": false,
        "bbox_hand_normalize_2d_joint": false
    },
    "model": {
        "name": "h2onet",
        "pretrain": true
    },
    "loss": {
        "name": "h2onet"
    },
    "metric": {
        "major_metric": "score"
    },
    "train": {
        "w_gr" : true,
        "num_epochs": 38,
        "batch_size": 32,
        "num_workers": 16
    },
    "test": {
        "batch_size": 64,
        "num_workers": 16
    },
    "summary": {
        "save_summary_steps": 50,
        "save_mid_freq": 10,
        "save_latest_freq": 1,
        "save_best_after": 0
    },
    "optimizer": {
        "name": "adam",
        "lr": 1e-4
    },
    "scheduler": {
        "name": "step",
        "milestones": [
            30
        ],
        "gamma": 0.1
    }
}
