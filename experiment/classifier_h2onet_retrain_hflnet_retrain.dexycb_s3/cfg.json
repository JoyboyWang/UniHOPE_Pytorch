{
    "base": {
        "exp_name": "classifier_h2onet_retrain_hflnet_retrain.dexycb_s3",
        "model_dir": "experiment/twoinone.dexycb/classifier_h2onet_retrain_hflnet_retrain.dexycb_s3"
    },
    "data": {
        "name": "DEX_YCB_HOPE",
        "transforms_type": "h2onet",
        "eval_type": [
            "test"
        ],
        "input_img_shape": [
            128,
            128
        ],
        "root_dir": "data/DexYCB",
        "data_split": "s3",
        "load_nori": false,
        "bbox_hand_normalize_2d_joint": true
    },
    "model": {
        "name": "twoinone_baseline_classifier",
        "cls_model": {
            "name": "classifier"
        },
        "h_model": {
            "name": "h2onet",
            "model": {
                "pretrain": false
            }
        },
        "ho_model": {
            "name": "hflnet"
        }
    },
    "loss": {
        "name": "placeholder"
    },
    "metric": {
        "major_metric": "score",
        "obj_eval_mode": "gt",
        "rep_threshold": 20,
        "add_threshold": 0.5
    },
    "train": {
        "num_epochs": 38,
        "batch_size": 64,
        "num_workers": 20
    },
    "test": {
        "batch_size": 64,
        "num_workers": 20
    },
    "summary": {
        "save_summary_steps": 50,
        "save_mid_freq": 10,
        "save_latest_freq": 1,
        "save_best_after": 0
    },
    "optimizer": {
        "name": "adam",
        "lr": 1e-3
    },
    "scheduler": {
        "name": "step",
        "milestones": [
            30
        ],
        "gamma": 0.1
    }
}
